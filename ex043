p = float(input('Qual o seu peso? '))
a = float(input('Qual sua altura em metros? '))
imc = p / (a * a)
if imc < 18.5:
    print('Abaixo do peso')
elif imc < 25:
    print('Peso ideal')
elif imc < 30:
    print('Sobrepeso')
elif imc < 40:
    print('Obesidade')
else:
    print('Obesidade mÃ³rbida')
