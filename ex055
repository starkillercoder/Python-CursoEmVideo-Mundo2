maior = menor = 0
for c in range(1, 6):
    p = float(input(f'Peso da {c}Âª pessoa: '))
    if c == 1:
        maior = p
        menor = p
    else:
        if p > maior:
            maior = p
        if p < menor:
            menor = p
print(f'Maior peso: {maior}kg')
print(f'Menor peso: {menor}kg')
